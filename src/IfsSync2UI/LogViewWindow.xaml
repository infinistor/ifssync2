<Window x:Class="IfsSync2UI.LogViewWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IfsSync2UI"
        mc:Ignorable="d"
        Closing="Window_Closing"
        Title="LogViewWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <TabControl x:Name="MainTab" Grid.Row="1" />
        <Button Click="Btn_SaveCSV" Content="Save csv" Width="55" Height="20" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="100, 0" />
        <Button Click="Btn_Refresh" Content="Refresh" Width="45" Height="20" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="50, 0" />
        <Button Click="Btn_Clear" Content="Clear" Width="45" Height="20" HorizontalAlignment="Right" VerticalAlignment="Top" />
    </Grid>
</Window>
